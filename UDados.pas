    { Private declarations }
  public
  var
      PATH_SERVER:String;
  end;

var
  FDados: TFDados;

implementation

{%CLASSGROUP 'Vcl.Controls.TControl'}

uses UFiscal, ULogin, Vcl.Dialogs, System.IniFiles;

{$R *.dfm}


procedure TFDados.DataModuleCreate(Sender: TObject);
var
  Config : TIniFile;
  DiretorioAtual : String;
begin

  DiretorioAtual := ExtractFilePath(ParamStr(0));
  if not (FileExists(DiretorioAtual + 'Config.ini')) then
    DiretorioAtual := 'C:\EasyControl\Fontes\';
  Config := TIniFile.Create(DiretorioAtual + 'Config.ini');
  PATH_SERVER := Config.ReadString('PATH_SERVER', 'Servidor', '');
  Config.Free;
  Dados.Params.Values['Database'] := PATH_SERVER;
  Dados.Connected := True;
end;

end.

end.
